<SCENE>

    <!-- Initial values -->
    <INITIALS>
        <frustum near="0.1" far="500"/>
        <translation x="-1" y="1" z="0" />
        <rotation axis="x" angle="0" />
        <rotation axis="y" angle="0" />
        <rotation axis="z" angle="0" />
        <scale sx="0.25" sy="0.25" sz="0.25" />
        <reference length="5" />
    </INITIALS>


    <!-- Illumination and clear colour -->
    <ILLUMINATION>
        <ambient r="0.2" g="0.2" b="0.2" a="1" />
        <background r="0" g="0" b="0" a="1" />
    </ILLUMINATION>


    <!-- Lights -->
    <LIGHTS>
        <LIGHT id="light0">
            <enable value ="1" />
            <position  x="15" y="10" z="0" w="1" />
            <ambient r="0.5" g="0.5" b="0.5" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </LIGHT>

        <LIGHT id="light1">
            <enable value ="1" />
            <position x="30" y="15" z="0" w="1" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="0.0" g="0.0" b="1.0" a="1.0" />
            <ambient r="0.0" g="0.0" b="1.0" a="1.0" />
        </LIGHT>

        <LIGHT id="light2">
            <enable value ="1" />
            <position x="15" y="15" z="15" w="1" />
            <ambient r="0.5" g="0.5" b="0.5" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
        </LIGHT>

        <LIGHT id="light3">
            <enable value ="1" />
            <position x="10" y="20" z="20" w="1" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="0.0" a="1.0" />
            <ambient r="1.0" g="1.0" b="0.0" a="1.0" />
        </LIGHT>

        <LIGHT id="light4">
            <enable value ="1" />
            <position x="5" y="9" z="25" w="1" />
            <ambient r="0.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="0.0" g="0.0" b="0.0" a="1.0" />
            <specular r="0.0" g="1.0" b="0.0" a="1.0" />
        </LIGHT>

    </LIGHTS>

    <!-- Textures -->
    <TEXTURES>

        <!-- Floor -->
        <TEXTURE id="floor">
            <file path="images/floor.png" />
            <amplif_factor s="1.0" t="1.0" />
        </TEXTURE>

        <!-- Window -->
        <TEXTURE id="window">
            <file path="images/window.png" />
            <amplif_factor s="12.0" t="5.8" />
        </TEXTURE>

        <!-- Wood -->
        <TEXTURE id="wood">
            <file path="images/wood.jpg" />
            <amplif_factor s="1.0" t="1.0" />
        </TEXTURE>

        <!-- Felt -->
        <TEXTURE id="tableFelt">
            <file path="images/felt.jpg" />
            <amplif_factor s="1.0" t="1.0" />
        </TEXTURE>

        <!-- White -->
        <TEXTURE id="white">
            <file path="images/white.jpg" />
            <amplif_factor s="1.0" t="1.0" />
        </TEXTURE>

        <!-- Dark Wood -->
        <TEXTURE id="darkWood">
            <file path="images/darkwood.jpg" />
            <amplif_factor s="1.0" t="1.0" />
        </TEXTURE>

        <!-- Blue Chalk -->
        <TEXTURE id="blueChalk">
            <file path="images/blueChalk.jpg" />
            <amplif_factor s="5.0" t="4.0" />
        </TEXTURE>

        <!-- Yellow Chalk -->
        <TEXTURE id="yellowChalk">
            <file path="images/yellowChalk.jpg" />
            <amplif_factor s="5.0" t="4.0" />
        </TEXTURE>

        <!-- Vending Machine -->
        <TEXTURE id="vendingMachineTexture">
            <file path="images/vendingMachine.jpg" />
            <amplif_factor s="1.0" t="1.0" />
        </TEXTURE>

        <!-- Glass -->
        <TEXTURE id="glass">
            <file path="images/glass.jpg" />
            <amplif_factor s="1.0" t="1.0" />
        </TEXTURE>

        <!-- Table -->
        <TEXTURE id="table">
            <file path="images/table.jpg" />
            <amplif_factor s="4.0" t="3.0" />
        </TEXTURE>

        <!-- Book Cover -->
        <TEXTURE id="bookCoverTexture">
            <file path="images/bookCover.png" />
            <amplif_factor s="4.0" t="5.0" />
        </TEXTURE>

        <!-- Book Pages -->
        <TEXTURE id="bookPagesTexture">
            <file path="images/bookPages.png" />
            <amplif_factor s="4.0" t="5.0" />
        </TEXTURE>

        <!-- Blue Tissue -->
        <TEXTURE id="blueTissue">
            <file path="images/tissue.jpg" />
            <amplif_factor s="1.0" t="1.0" />
        </TEXTURE>

        <!-- triangle -->
        <TEXTURE id="triangle">
            <file path="images/rgb-triangle.jpg" />
            <amplif_factor s="4.0" t="3.0" />
        </TEXTURE>

    </TEXTURES>


    <MATERIALS>

        <MATERIAL id="ballRed">
            <shininess value="30" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="1.0" g="0.0" b="0.0" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
        </MATERIAL>

        <MATERIAL id="ballBlack">
            <shininess value="30" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.0" g="0.0" b="0.0" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
        </MATERIAL>

        <MATERIAL id="ballWhite">
            <shininess value="30" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />
            <ambient r="1.0" g="1.0" b="1.0" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
        </MATERIAL>

        <MATERIAL id="ballBlue">
            <shininess value="30" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="0.0" g="0.0" b="1.0" a="1.0" />
            <ambient r="0.0" g="0.0" b="1.0" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
        </MATERIAL>

        <MATERIAL id="ballGreen">
            <shininess value="30" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="0.0" g="1.0" b="0.0" a="1.0" />
            <ambient r="0.0" g="1.0" b="0.0" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
        </MATERIAL>

        <MATERIAL id="ballYellow">
            <shininess value="30" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="1.0" b="0.0" a="1.0" />
            <ambient r="1.0" g="1.0" b="0.0" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
        </MATERIAL>

        <MATERIAL id="ballBrown">
            <shininess value="30" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="0.55" g="0.27" b="0.08" a="1.0" />
            <ambient r="0.55" g="0.27" b="0.08" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
        </MATERIAL>

        <MATERIAL id="ballPink">
            <shininess value="30" />
            <specular r="1.0" g="1.0" b="1.0" a="1.0" />
            <diffuse r="1.0" g="0.4" b="0.7" a="1.0" />
            <ambient r="1.0" g="0.4" b="0.7" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
        </MATERIAL>

        <MATERIAL id="tableGreen">
            <shininess value="5" />
            <specular r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="0.06" g="0.44" b="0.3" a="1.0" />
            <ambient r="0.06" g="0.44" b="0.3" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
        </MATERIAL>

        <MATERIAL id="darkHole">
            <shininess value="5" />
            <specular r="0.1" g="0.1" b="0.1" a="1.0" />
            <diffuse r="0.05" g="0.05" b="0.05" a="1.0" />
            <ambient r="0.0" g="0.0" b="0.0" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
        </MATERIAL>

        <MATERIAL id="black">
            <shininess value="10" />
            <specular r="0.0" g="0.0" b="1.0" a="1.0" />
            <diffuse r="0.0" g="0.0" b="0.0" a="1.0" />
            <ambient r="0.0" g="0.0" b="0.0" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="0.0" />
        </MATERIAL>

        <!-- A default material -->
        <MATERIAL id="defaultMaterial">
            <shininess value="10" />
            <specular r="0.0" g="0.0" b="0.0" a="1.0" />
            <diffuse r="0.4" g="0.4" b="0.4" a="1.0" />
            <ambient r="0.4" g="0.4" b="0.4" a="1.0" />
            <emission r="0.0" g="0.0" b="0.0" a="1.0" />
        </MATERIAL>



    </MATERIALS>

    <!-- Animations -->

    <ANIMATIONS>

        <ANIMATION id="BezierAnimation1" speed="10" type="bezier">
            <controlpoint xx="0.0" yy="0.0" zz="0.0"></controlpoint>
            <controlpoint xx="0.0" yy="10.0" zz="-10.0"></controlpoint>
            <controlpoint xx="0.0" yy="12.0" zz="-20.0"></controlpoint>
            <controlpoint xx="0.0" yy="0.0" zz="-40.0"></controlpoint>
        </ANIMATION>

        <ANIMATION id="BezierAnimation2" speed="10" type="bezier">
            <controlpoint xx="0.0" yy="0.0" zz="-20.0"></controlpoint>
            <controlpoint xx="-5.0" yy="7.0" zz="-20.0"></controlpoint>
            <controlpoint xx="-15.0" yy="9.0" zz="-20.0"></controlpoint>
            <controlpoint xx="-25.0" yy="0.0" zz="-20.0"></controlpoint>
        </ANIMATION>

        <ANIMATION id="LinearAnimation1" speed="10" type="linear">
            <controlpoint xx="0.0" yy="0.0" zz="0.0"></controlpoint>
            <controlpoint xx="20.0" yy="0.0" zz="0.0"></controlpoint>
            <controlpoint xx="20.0" yy="0.0" zz="-10.0"></controlpoint>
        </ANIMATION>

        <ANIMATION id="LinearAnimation2" speed="10" type="linear">
            <controlpoint xx="20.0" yy="0.0" zz="0.0"></controlpoint>
            <controlpoint xx="40.0" yy="0.0" zz="0.0"></controlpoint>
        </ANIMATION>

        <ANIMATION id="ComboAnimation1" type="combo">
            <SPANREF id="BezierAnimation1"/>
            <SPANREF id="BezierAnimation2"/>
            <SPANREF id="LinearAnimation1"/>
            <SPANREF id="LinearAnimation2"/>
        </ANIMATION>

        <ANIMATION id="LinearAnimationExample" speed="5" type="linear">
            <controlpoint xx="0.0" yy="0.0" zz="0.0"></controlpoint>
            <controlpoint xx="0.0" yy="10.0" zz="5.0"></controlpoint>
            <controlpoint xx="0.0" yy="10.0" zz="10.0"></controlpoint>
            <controlpoint xx="0.0" yy="0.0" zz="15.0"></controlpoint>
        </ANIMATION>
        <ANIMATION id="LinearAnimationExample2" speed="5" type="linear">
            <controlpoint xx="0.0" yy="0.0" zz="0.0"></controlpoint>
            <controlpoint xx="0.0" yy="10.0" zz="5.0"></controlpoint>
            <controlpoint xx="0.0" yy="10.0" zz="10.0"></controlpoint>
            <controlpoint xx="0.0" yy="0.0" zz="15.0"></controlpoint>
        </ANIMATION>

        <ANIMATION id="CircularAnimationExample" speed="5" type="circular"
                   centerx="0.0" centery="0.0" centerz="0.0"
                   radius="5" startang="0.0" rotang="360"/>

        <ANIMATION id="BezierAnimationExample" speed="5" type="bezier">
            <controlpoint xx="0.0" yy="0.0" zz="0.0"></controlpoint>
            <controlpoint xx="0.0" yy="10.0" zz="5.0"></controlpoint>
            <controlpoint xx="0.0" yy="10.0" zz="10.0"></controlpoint>
            <controlpoint xx="0.0" yy="0.0" zz="15.0"></controlpoint>
        </ANIMATION>

        <ANIMATION id="ComboAnimationExample" type="combo">
            <SPANREF id="LinearAnimationExample2"/>
            <SPANREF id="LinearAnimationExample2"/>
        </ANIMATION>

        <ANIMATION id="AnotherComboAnimationExample" type="combo">
            <SPANREF id="CircularAnimationExample"/>
            <SPANREF id="LinearAnimationExample"/>
        </ANIMATION>

    </ANIMATIONS>



    <!-- The leaves -->
    <LEAVES>
    </LEAVES>


    <!-- The nodes -->
    <NODES>

        <ROOT id="root" />

        <!-- The root node -->
        <NODE id="root">
            <MATERIAL id="defaultMaterial" />
            <TEXTURE id="null" />

            <DESCENDANTS>
                <NODEREF id="floor" />
                <NODEREF id="poolTable" />
                <NODEREF id="vendingMachine" />
                <NODEREF id="smallTable" />
                <NODEREF id="window" />
            </DESCENDANTS>

        </NODE>

        <!-- The floor -->
        <NODE id="floor">
            <MATERIAL id="null" />
            <TEXTURE id="floor" />

            <SCALE sx="10" sy="1" sz="8" />
            <TRANSLATION x="0" y="0" z="4" />
            <ROTATION axis="x" angle="-90" />

            <DESCENDANTS>
                <LEAF  type="rectangle" args="0 4 5 0" />
            </DESCENDANTS>

        </NODE>

        <!-- Window -->
        <NODE id="window" selectable="true">
            <MATERIAL id="null" />
            <TEXTURE id="window" />

            <TRANSLATION x="0" y="4" z="30" />
            <ROTATION axis="y" angle="90" />

            <DESCENDANTS>
                <NODEREF id="curtains" />
                <LEAF  type="rectangle" args="0 6 12 0" />
            </DESCENDANTS>

        </NODE>

        <!-- The pool table -->
        <NODE id="poolTable" selectable="true">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <DESCENDANTS>
                <NODEREF id="legs" />
                <NODEREF id="top" />
            </DESCENDANTS>

        </NODE>

        <!-- The legs of the table -->
        <NODE id="legs">
            <MATERIAL id="null" />
            <TEXTURE id="wood" />

            <DESCENDANTS>
                <NODEREF id="leg1" />
                <NODEREF id="leg2" />
                <NODEREF id="leg3" />
                <NODEREF id="leg4" />
            </DESCENDANTS>

        </NODE>

        <!-- One leg -->
        <NODE id="leg1">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <DESCENDANTS>
                <NODEREF id="leg" />
            </DESCENDANTS>

        </NODE>

        <!-- Another leg -->
        <NODE id="leg2">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <TRANSLATION x="15" y="0" z="0" />

            <DESCENDANTS>
                <NODEREF id="leg" />
            </DESCENDANTS>

        </NODE>

        <!-- Another leg -->
        <NODE id="leg3">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <TRANSLATION x="0" y="0" z="10" />

            <DESCENDANTS>
                <NODEREF id="leg" />
            </DESCENDANTS>

        </NODE>

        <!-- Another leg -->
        <NODE id="leg4">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <TRANSLATION x="15" y="0" z="10" />

            <DESCENDANTS>
                <NODEREF id="leg" />
            </DESCENDANTS>

        </NODE>

        <!-- Generic leg -->
        <NODE id="leg">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <TRANSLATION x="8.5" y="0" z="5" />
            <SCALE sx="0.5" sy="1" sz="0.5" />
            <ROTATION axis="x" angle="-90" />

            <DESCENDANTS>
                <LEAF id="cylinder1" type="cylinder" args="4 1 1 20 20 1 1" />
            </DESCENDANTS>

        </NODE>

        <!-- The top of the table -->
        <NODE id="top">
            <MATERIAL id="null" />
            <TEXTURE id="wood" />

            <DESCENDANTS>
                <NODEREF id="base" />
                <NODEREF id="cue" />
                <NODEREF id="balls" />
                <NODEREF id="holes" />
                <NODEREF id="partitions" />
                <NODEREF id="chalk" />
            </DESCENDANTS>

        </NODE>

        <!-- The base of the top of the table -->
        <NODE id="base">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <SCALE sx="3.3" sy="0.25" sz="2.2" />
            <TRANSLATION x="-0.15" y="10" z="-0.45" />

            <DESCENDANTS>
                <NODEREF id="faceBase1" />
                <NODEREF id="faceBase2" />
                <NODEREF id="faceBase3" />
                <NODEREF id="faceBase4" />
                <NODEREF id="faceBase5" />
                <NODEREF id="faceBase6" />
            </DESCENDANTS>

        </NODE>

        <!-- A base face -->
        <NODE id="faceBase1">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <TRANSLATION x="2.5" y="4" z="7.5" />

            <DESCENDANTS>
                <LEAF  type="rectangle" args="0 4 5 0" />
            </DESCENDANTS>

        </NODE>

        <!-- Another base face -->
        <NODE id="faceBase2">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <TRANSLATION x="2.5" y="4" z="2.5" />
            <ROTATION axis="y" angle="-90" />

            <DESCENDANTS>
                <LEAF  type="rectangle" args="0 4 5 0" />
            </DESCENDANTS>

        </NODE>

        <!-- Another base face -->
        <NODE id="faceBase3">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <TRANSLATION x="7.5" y="4" z="7.5" />
            <ROTATION axis="y" angle="90" />

            <DESCENDANTS>
                <LEAF  type="rectangle" args="0 4 5 0" />
            </DESCENDANTS>

        </NODE>

        <!-- Another base face -->
        <NODE id="faceBase4">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <TRANSLATION x="7.5" y="4" z="2.5" />
            <ROTATION axis="y" angle="180" />

            <DESCENDANTS>
                <LEAF  type="rectangle" args="0 4 5 0" />
            </DESCENDANTS>

        </NODE>

        <!-- Another base face -->
        <NODE id="faceBase5">
            <MATERIAL id="tableGreen" />
            <TEXTURE id="tableFelt" />

            <ROTATION axis="x" angle="-90" />
            <TRANSLATION x="2.5" y="-7.5" z="8" />
            <SCALE sx="1" sy="1.25" sz="1" />

            <DESCENDANTS>
                <LEAF  type="rectangle" args="0 4 5 0" />
            </DESCENDANTS>

        </NODE>

        <!-- Another base face -->
        <NODE id="faceBase6">
            <MATERIAL id="null" />
            <TEXTURE id="null" />

            <TRANSLATION x="2.5" y="4" z="2.5" />
            <ROTATION axis="x" angle="90" />
            <SCALE sx="1" sy="1.25" sz="1" />

            <DESCENDANTS>
                <LEAF  type="rectangle" args="0 4 5 0" />
            </DESCENDANTS>

        </NODE>


        <!-- The cue -->
        <NODE id="cue">
            <MATERIAL id="null" />
            <TEXTURE id="wood" />

            <SCALE sx="0.2" sy="0.2" sz="2" />
            <TRANSLATION x="100" y="24" z="3" />

            <DESCENDANTS>
                <NODEREF id="cueBody" />
                <NODEREF id="cueTop" />
                <NODEREF id="cueTip" />
                <NODEREF id="cueBack" />
            </DESCENDANTS>

        </NODE>

        <!-- The body of the cue -->
        <NODE id="cueBody">
            <MATERIAL id="null" />
            <TEXTURE id="wood" />

            <DESCENDANTS>
                <LEAF id="cylinder1" type="cylinder" args="4 1 1.5 20 20 1 1" />
            </DESCENDANTS>

        </NODE>

        <!-- The top of the cue -->
        <NODE id="cueTop">
            <MATERIAL id="null" />
            <TEXTURE id="white" />

            <SCALE sx="0.8" sy="0.8" sz="0.05" />
            <TRANSLATION x="0" y="0" z="-4.2" />

            <DESCENDANTS>
                <LEAF id="cylinder1" type="cylinder" args="4 1 1 20 20 1 1" />
            </DESCENDANTS>

        </NODE>

        <!-- The tip of the cue -->
        <NODE id="cueTip">
            <MATERIAL id="null" />
            <TEXTURE id="wood" />

            <SCALE sx="0.8" sy="0.8" sz="0.02" />
            <TRANSLATION x="0" y="0" z="-15" />

            <DESCENDANTS>
                <LEAF id="cylinder1" type="cylinder" args="4 1 1 20 20 1 1" />
            </DESCENDANTS>

        </NODE>

        <!-- The back of the cue -->
        <NODE id="cueBack">
            <MATERIAL id="null" />
            <TEXTURE id="darkWood" />

            <TRANSLATION x="0" y="0" z="4" />
            <SCALE sx="0.7" sy="0.7" sz="0.07" />

            <DESCENDANTS>
                <LEAF id="sphere1" type="sphere" args="2 20 20" />
            </DESCENDANTS>

        </NODE>

        <!-- The balls -->
        <NODE id="balls">
            <MATERIAL id="null" />
            <TEXTURE id="clear" />

            <DESCENDANTS>
                <NODEREF id="ball1" />
                <NODEREF id="ball2" />
                <NODEREF id="ball3" />
                <NODEREF id="ball4" />
                <NODEREF id="ball5" />
                <NODEREF id="ball6" />
                <NODEREF id="ball7" />
                <NODEREF id="ball8" />
                <NODEREF id="ball9" />
            </DESCENDANTS>

        </NODE>

        <!-- One ball -->
        <NODE id="ball1">
            <MATERIAL id="ballBlack" />
